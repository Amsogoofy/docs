{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Leafy",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "icon": "book-open",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "quickstart",
              "development",
              "navigation"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "icon": "square-terminal",
        "groups": [
          {
            "group": "Endpoints",
            "pages": [
              "api-reference/get",
              "api-reference/post",
              "api-reference/delete"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Blog",
          "icon": "newspaper",
          "href": "https://external-link.com/blog"
        }
